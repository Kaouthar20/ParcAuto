<h1>liste des produits</h1>
{% block body %}
	<div class="container mt-5">
		<table class="table table-striped table-hover">
			<thead class="bg-info">
				<tr>
					<th>ID</th>
					<th>Designation</th>
					<th>quantite</th>


				</tr>
			</thead>
			<tbody>
				{% for p in products %}
					<tr>
						<td>{{p.id}}</td>
						<td>{{p.designation}}</td>
						<td>{{p.qte}}</td>


					</tr>

				{% endfor %}

			</tbody>
		</table>
	</div>

{% endblock %}
